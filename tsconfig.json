// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@styles": [
        "./src/styles"
      ],
      "@images": [
        "./src/assets/images"
      ],      
      "@icons": [
        "./src/assets/icons"
      ],      
      "@atoms/*": [
        "./src/components/atoms/*"
      ],      
      "@molecules/*": [
        "./src/components/molecules/*"
      ],      
      "@organisms/*": [
        "./src/components/organisms/*"
      ],            
      "@views/*": [
        "./src/views/*"
      ],      
      "@utils/*": [
        "./src/utils/*"
      ],      
      "@app/*": [
        "./src/*"
      ],
    } ,
    "skipLibCheck": true  
  }
}
